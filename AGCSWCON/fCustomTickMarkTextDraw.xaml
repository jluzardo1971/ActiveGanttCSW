        xmlns:AGCSW="clr-namespace:AGCSW;assembly=AGCSW"
        Title="Custom TickMark Text Draw" Height="470" Width="780" Background="#FFFFFFFF" Loaded="Window_Loaded">
    <Border Margin="10,10,10,10" BorderBrush="White" BorderThickness="1">
        <Grid Margin="10,10,10,10" Name="AGContainerGrid">
            <AGCSW:ActiveGanttCSWCtl Name="ActiveGanttCSWCtl1" TickMarkTextDraw="ActiveGanttCSWCtl1_TickMarkTextDraw" />
        </Grid>
    </Border>
</Window>